{
    "env":{
        "browser": true,
        "es2021": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module",
        "parser": "@babel/eslint-parser",
        "requireConfigFile": false
    },
    "plugins": [
        "vue"
    ],
    "extends": [
        "airbnb-base",
        "plugin:vue/vue3-recommended",
        "prettier",
        "plugin:import/errors",
        "plugin:import/warnings",
        "plugin:import/typescript"

    ],
    "rules": {
        "vue/multi-word-component-names": "warn"
    }
}